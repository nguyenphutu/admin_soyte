@model List<admin.tandienbien.gov.vn.Models.STATISTICS_VISITOR>

@{
    ViewBag.Title = "Quản lý danh sách truy cập";
}
<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">Quản lý danh sách truy cập</h2>

    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Danh sách truy cập "http://tanddienbien.gov.vn"
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th style="width:10%; text-align:center;">STT</th>
                            <th style="width:25%; text-align:center;">IP truy cập</th>
                            <th style="width:65%; text-align:center;">Thông tin thiết bị</th>
                            <th style="width:25%; text-align:center;">Thông tin Ứng dụng</th>
                            <th style="width:25%; text-align:center;">Số lần truy cập</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            foreach (var st in Model)
                            {
                                <tr class="odd gradeX">
                                    <td align="center">@st.ID</td>
                                    <td>@st.IP_ADDRESS</td>
                                    <td>@st.AGENT</td>
                                    <td>@st.PLATFORM</td>
                                    <td>@st.COUNT</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

